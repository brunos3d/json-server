{
    "name": "express-typescript-docker-template",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev": "tsnd --transpile-only --respawn --no-notify --ignore-watch node_modules ./src/index.ts",
        "start": "node ./dist/index.js",
        "build": "yarn tsc",
        "deploy": "yarn && yarn build && yarn start"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.13",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.3.5"
    }
}
